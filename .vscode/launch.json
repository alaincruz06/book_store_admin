{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Web Debug - Dev",
            "request": "launch",
            "type": "dart",
            "flutterMode": "debug",
            "program": "lib/main.dart",
            "args": [
                "--web-port",
                "54287",
                "--web-browser-flag",
                "--disable-web-security",
            ]
        },
        {
            "name": "Web Debug - Prod",
            "request": "launch",
            "type": "dart",
            "flutterMode": "debug",
            "program": "lib/main.dart",
            "args": [
                "--web-port",
                "54289",
                "--web-browser-flag",
                "--disable-web-security",
            ]
        },
        {
            "name": "Web Release - Prod",
            "request": "launch",
            "type": "dart",
            "flutterMode": "release",
            "program": "lib/main.dart",
            "args": [
                "--web-port",
                "54290",
            ]
        }
    ]
}